Algoritmo Ejercicio3
	
	Definir num1, num2 Como Entero;
	num1 = 0;
	num2 = 0;
	
	Escribir "Escribe dos números";
	Leer num1;
	Leer num2;
	
	//Operadores Lógicos: <, >, =, <>, >=, <=
	Si (num1 < num2) Entonces
		Escribir "El mayor es " num2;
	Fin Si;
	
	Si (num1 > num2) Entonces
		Escribir "El mayor es " num1;
	Fin Si;
	
	Si (num1 == num2) Entonces;
		Escribir "Ambos son iguales";
	FinSi;
	
FinAlgoritmo
