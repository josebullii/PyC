Algoritmo Ejercicio5
	
	Definir num, multiplicacion, contador como Real;
	num = 0;
	multiplicacion = 0;
	contador = 0;
	
	Escribir "Dime un número";
	Leer num;
	
	Repetir
		
		multiplicacion = num * contador;
		contador = contador + 1;
		
		Escribir "La tabla del " num " es: " multiplicacion;
		
	Hasta Que contador = 10
	
	
FinAlgoritmo
